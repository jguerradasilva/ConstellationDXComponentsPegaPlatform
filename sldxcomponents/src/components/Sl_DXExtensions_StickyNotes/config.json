{
  "name": "Sl_DXExtensions_StickyNotes",
  "label": "Sticky Notes",
  "description": "A sticky notes widget to create, edit, and delete notes with customizable colors",
  "organization": "Sl",
  "version": "1.0.0",
  "library": "DXExtensions",
  "allowedApplications": [],
  "componentKey": "Sl_DXExtensions_StickyNotes",
  "type": "Widget",
  "subtype": ["PAGE", "CASE"],
  "icon": "OneColumnPage.svg",
  "properties": [
    {
      "name": "label",
      "label": "Label",
      "format": "TEXT",
      "defaultValue": "Sticky Notes"
    },
    {
      "name": "value",
      "label": "Notes List Property",
      "format": "TEXT",
      "defaultValue": ".NotesList"
    },
    {
      "name": "readOnly",
      "label": "Read Only",
      "format": "BOOLEAN",
      "defaultValue": false
    },
    {
      "name": "visibility",
      "label": "Visible",
      "format": "VISIBILITY"
    },
    {
      "label": "Advanced",
      "format": "GROUP",
      "collapsible": true,
      "properties": [
        {
          "name": "testId",
          "label": "Test ID",
          "format": "TEXT",
          "ignorePattern": "[^-_\\p{N}\\p{L}]",
          "includeAnnotations": false
        }
      ]
    }
  ],
  "defaultConfig": {
    "label": "Sticky Notes",
    "value": ".NotesList",
    "readOnly": false
  }
}
